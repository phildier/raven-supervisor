[program:<%=@name%>]
command=<%=@command%>
directory=<%=@directory%>
user=<%=@user%>
<% if @numprocs > 1 %>
numprocs=<%=@numprocs%>
process_name=%(process_num)s
<% end %>
<% if @environment.length > 0 %>
environment=<%=@environment.map { |k,v| "#{k.to_s}=\"#{v.to_s}\"" }.join ","%>
<% end %>
autostart=<%=@autostart ||= "true"%>
autorestart=<%=@autorestart ||= "true"%>
exitcodes=<%=@exitcodes.join(",")%>
stdout_logfile=/var/log/<%=@name%>.log
redirect_stderr=true
stopsignal=<%=@stopsignal ||= "QUIT"%>
stopwaitsecs=<%=@stopwaitsecs ||= "10"%>
